    "apollo-link": "^1.2.0",
    "apollo-link-ws": "^1.0.19",
    "chart.js": "^2.9.3",
    "classlist.js": "1.1.20150312",
    "core-js": "^2.5.4",
    "cryptojs": "^2.5.3",
